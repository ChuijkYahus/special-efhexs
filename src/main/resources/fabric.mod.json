{
	"schemaVersion": 1,
	"id": "efhexs",
	"version": "${version}",
	"name": "Special Efhexs",
	"description": "A Hexcasting addon about recording and playing back particles and sounds.",
	"authors": [
		"miyucomics"
	],
	"contact": {
		"homepage": "https://github.com/miyucomics/efhexs",
		"sources": "https://github.com/miyucomics/efhexs"
	},
	"license": "MIT",
	"icon": "assets/efhexs/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"adapter": "kotlin",
				"value": "miyucomics.efhexs.EfhexsMain"
			}
		],
		"client": [
			{
				"adapter": "kotlin",
				"value": "miyucomics.efhexs.EfhexsClient"
			}
		]
	},
	"mixins": [
		"efhexs.mixins.json"
	],
	"depends": {
		"minecraft": "1.20.1",
		"fabricloader": ">=0.16.7",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.10.10+kotlin.1.9.10",
		"hexcasting": ">=0.11.1-7-pre-646",
		"hexpose": ">=1.0.0"
	}
}
